  </file>

  <file id="emiss_om_biomass_hi_ancil" name="emiss_om_biomass_hi_ancil" mode="read" output_freq="1mo" convention="UGRID" cyclic="true" enabled=".FALSE.">
    <field id="em_om_bb_hi_time" name="time" unit="days" operation="once" axis_ref="emiss_axis"/>
    <field id="emiss_om_biomass_high_data" name="emissions_OC_biomass_high" long_name="OC biomass high-level emissions expressed as carbon" unit="kg(C) m-2 s-1" operation="instant" domain_ref="face" axis_ref="emiss_axis"/>
